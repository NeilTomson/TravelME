<div class="form-group">
  <label for="title">title</label>
  <input
    type="text"
    name="title"
    id="title"
    class="form-control"
    value="<%= travel.title %> "
    required
  />
</div>
<div class="form-group">
  <label for="descrition">descrition</label>
  <textarea
    type="text"
    name="descrition"
    id="descrition"
    class="form-control"
    required
  >
<%= travel.descrition %> 
  </textarea>
</div>
<div class="form-group">
  <div class="d-flex text-center">
    <div class="mx-4">
      <label for="lat">latitude</label>
      <input
        type="text"
        name="lat"
        id="lat"
        class="form-control"
        value="<%= travel.lat %> "
        data-lat="<%= travel.lat %>"
        required
      />
    </div>
    <div>
      <label for="lng">longitude</label>
      <input
        type="text"
        name="lng"
        id="lng"
        class="form-control"
        data-lng="<%= travel.lng %>"
        value="<%= travel.lng %> "
        required
      />
    </div>
  </div>
  <div></div>
</div>
<div class="d-flex m-2">
  <a class="btn btn-secondary mx-4" href="/">Cancel</a>
  <button type="submit" class="btn btn-primary">Save</button>
</div>
